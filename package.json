  "license": "MIT",
  "bugs": {
  },
  "homepage": "https://johnnywhichard.com",
  "devDependencies": {
    "autoprefixer": "^9.0.1",
    "babel-core": "^6.26.3",
    "babel-preset-env": "^1.7.0",
    "browser-sync": "^2.24.6",
    "eslint-config-google": "*",
    "eslint-loader": "^2.1.0",
    "gulp": "^3.9.1",
    "gulp-babel": "*",
    "gulp-eslint": "^5.0.0",
    "gulp-imagemin": "*",
    "gulp-newer": "^1.4.0",
    "gulp-plumber": "^1.2.0",
    "gulp-postcss": "*",
    "gulp-sass": "^4.0.1",
